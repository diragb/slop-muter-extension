  "private": true,
  "version": "0.1.0",
  "type": "module",
  "author": "Dirag Biswas (https://github.com/diragb)",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/diragb"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diragb/slop-muter-extension.git"
  },
  "bugs": {
    "url": "https://github.com/diragb/slop-muter-extension/issues"
  },
  "homepage": "https://github.com/diragb/slop-muter-extension#readme",
  "scripts": {
